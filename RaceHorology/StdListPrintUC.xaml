<UserControl x:Class="RaceHorology.StdListPrintUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RaceHorology"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="500">
  <WrapPanel>
    <CheckBox x:Name="cbWithRaceHeader" Content="Mit Rennkopf" Grid.Row="0" Margin="5 0"/>
    <CheckBox x:Name="cbWithDiagram" Content="Mit Ergebnisgraph" Grid.Row="1" Margin="5 0"/>
    <CheckBox x:Name="cbOneGroupPerPage" Content="Neue Seite beim Gruppenwechsel" Grid.Row="2" Margin="5 0"/>   
  </WrapPanel>
</UserControl>
